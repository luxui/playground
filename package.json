{
  "name": "playground",
  "version": "1.0.0",
  "description": "A demo application to introduce functionality in LuxUI.",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "prebuild": "npm run htmlcss",
    "htmlcss": "cp ./src/index.html ./build/index.html && cp ./src/styles.css ./build/styles.css",
    "prehtmlcss": "rm -rf build && mkdir build",
    "server": "pm2 start server/api.js --name=\"luxui-demo-api\" --watch",
    "start": "webpack-dev-server --hot",
    "prestart": "npm run htmlcss",
    "stop": "pm2 stop server/api.js",
    "fetch-local": "cp ../lux-core/dist/reactLux.js node_modules/@luxui/lux/dist/reactLux.js"
  },
  "keywords": [],
  "author": "Joshua T Kalis <JoshKalis@QuickenLoans.com>",
  "license": "MIT",
  "devDependencies": {},
  "dependencies": {
    "@luxui/lux": "^1.0.0-alpha-7",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "exports-loader": "^0.6.3",
    "imports-loader": "^0.6.5",
    "pm2": "^2.6.1",
    "react": "^15.4.2",
    "react-a11y": "^0.3.3",
    "react-dom": "^15.4.2",
    "redux": "^3.6.0",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^1.16.2"
  }
}
